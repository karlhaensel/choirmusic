tBasseins = \lyricmode {
  \set stanza = "1." Al -- le Völ -- ker weit und breit
  lo -- bet Gott in E -- wig -- keit.
}

tBasszwei = \lyricmode {
  \set stanza = "2." Prei -- set den in sei -- ner Macht,
